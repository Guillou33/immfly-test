{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "incremental": true,
    "ignoreDeprecations": "6.0",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./app/*"
      ],
      "@/Store/*": [
        "./Store/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/constants/*": [
        "./constants/*"
      ],
      "@/lib/*": [
        "./lib/*"
      ],
      "@/api/*": [
        "./api/*"
      ]
    },
    "types": [
      "jest",
      "react-native-vector-icons"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "metro.config.js",
    "babel.config.js"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
